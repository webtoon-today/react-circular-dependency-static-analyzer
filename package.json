    "web": "node src/server.js",
    "web:dev": "nodemon src/server.js",
    "test": "jest",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webtoon-today/react-circular-dependency-static-analyzer.git"
  },
  "bugs": {
