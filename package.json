  },
  "homepage": "https://github.com/webtoon-today/react-circular-dependency-static-analyzer#readme",
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
